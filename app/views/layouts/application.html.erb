<!DOCTYPE html>
<html>
  <head>
    <title>SidekiqMailer</title>
    <%= stylesheet_link_tag    "application", :media => "all" %>
    <%= javascript_include_tag "application" %>
    <%= csrf_meta_tags %>
  </head>
  <body data-twttr-rendered="true">
    <%= render '/layouts/shared/header_menu' %>
    <div class="container">
      <br/>
      <br/>

      <% !flash.empty? && flash.each do |name, msg| %>
        <div class=<%= flash_level(name) %>>
          <%= msg %>
        </div>
      <% end %>

      <%= yield %>
    </div>
  </body>
</html>
